getgenv().Settings = {
       Toggle_ClickAutoClick = nil, 
       Button_ResetClickPosition = nil,
       ToggleESPNPCRealXtremeV2 = nil,
       ToggleGodESP = nil,
       ToggleUltraSpeed = nil,
       ToggleAntiESP = nil,
