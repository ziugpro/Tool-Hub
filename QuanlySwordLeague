                        StrokeFrame.Parent = BackgroundFrame

                        local PlayerName = Instance.new("TextLabel")
                        PlayerName.Name = "PlayerName"
                        PlayerName.Size = UDim2.new(1, 0, 0.33, 0)
                        PlayerName.Position = UDim2.new(0, 0, 0, 0)
